{
  "name": "heyseli",
  "version": "0.0.1",
  "description": "My personal website",
  "repository": {
    "type": "git",
    "url": "https://github.com/hyperh/heyseli.git"
  },
  "bugs": {
    "url": "https://github.com/hyperh/heyseli/issues"
  },
  "homepage": "https://github.com/hyperh/heyseli",
  "scripts": {
    "develop": "gatsby develop",
    "start": "webpack-dev-server --open --config webpack.dev.js",
    "build:prod": "webpack --config webpack.prod.js",
    "eslint": "eslint src",
    "clean": "rimraf ./dist",
    "prettier:fix": "prettier --write 'src/**/*.js'",
    "copy": "cp index.prod.html dist/index.html && cp CNAME dist/CNAME",
    "watch:test": "npm test -- --auto-watch --no-single-run",
    "deploy": "npm run build:prod && npm run copy && gh-pages -d dist --repo https://$GITHUB_TOKEN@github.com/hyperh/heyseli.git",
    "test": "yarn run eslint"
  },
  "dependencies": {
    "gatsby": "^2.21.33",
    "gatsby-source-filesystem": "^2.3.7",
    "lodash": "^4.17.13",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-icons": "^3.10.0",
    "react-images": "^1.1.7",
    "styled-components": "^5.1.0"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "eslint": "^7.0.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.20.0",
    "gh-pages": "^2.2.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.2",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "eslint",
      "git add"
    ]
  }
}
